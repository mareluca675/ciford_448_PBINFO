#include <iostream>

using namespace std;

int main()
{
    int a, b, c, n;
    
    cin >> n;
    
    a = n % 10;
    b = n / 10 % 10;
    c = n / 100;
    
    if(a > b)
    {
        swap(a, b);
    }
    
    if(a > c)
    {
        swap(a, c);
    }
    
    if(b > c)
    {
        swap(b, c);
    }
    
    cout << a << ' ' << b << ' ' << c;

    return 0;
}
